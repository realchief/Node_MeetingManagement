{{> head }}

<body class="home">

	<div class="app-container">

		<div class="section header clearfix">
			{{> header }}
		</div>
		<div class="below-header"><div class="below-header-inside">
			<div class="container">
				
				

				<div class="google-selected-accounts">
					
					{{#if linkedAccounts.googleAccount }}		

					<h2 class="with-line"><i class="fab fa-google"></i> Google Analytics</h2>	
					<h2 class="label fb-account">Connected as: {{linkedAccounts.googleAccount.email}}</h2>		
					<h2 class="select-view-google-text label ga-account">
						To Account: {{linkedAccounts.googleAccount.account_name}} : {{linkedAccounts.googleAccount.property_name}} - {{linkedAccounts.googleAccount.view_name}}
					</h2>
					{{toJSON linkedAccounts.googleAccount}}
					{{/if}}

					{{#if linkedAccounts.facebookAccount }}			
					<h2 class="with-line"><i class="fab fa-facebook"></i> Facebook</h2>	
					<h2 class="label fb-account">Connected as: {{linkedAccounts.facebookAccount.email}}</h2>
					<h2 class="select-view-facebook-text label fb-account">
						To Account: {{linkedAccounts.facebookAccount.account_name}}
					</h2>
					{{toJSON linkedAccounts.facebookAccount}}
					{{/if}}

				</div>


			</div>
			<div class="section gutter footer">
				{{> footer }}
			</div>
		</div>
	</div>	
</div>

<div class="test"></div>
	
	{{> scripts }}


</body>

</html>
